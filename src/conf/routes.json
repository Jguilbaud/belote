{
	"home": {
		"http_method": "get",
		"type": "page",
		"regexp": "^/$",
		"controller": "Home",
		"method": "showHomePage",
		"params": {
			
		}
	},
	"createpage": {
		"http_method": "get",
		"type": "page",
		"regexp": "/create",
		"controller": "Game",
		"method": "create",
		"params": {
			
		}
	},
	"creategame": {
		"http_method": "post",
		"type": "page",
		"regexp": "/create",
		"controller": "Game",
		"method": "create",
		"params": {
			"pseudo": {
				"method": "post"
			}
		}
	},
	"joingamepage": {
		"http_method": "get",
		"type": "page",
		"regexp": "^/join/([a-z0-9]{10})$",
		"controller": "Game",
		"method": "showJoinPage",
		"params": {
			"hash": {
				"method": "uri",
				"uri_position": 1
			}
		}
	},
    "joingameapi": {
        "http_method": "post",
        "type": "api",
        "regexp": "^/join/([a-z0-9]{10})$",
        "controller": "Game",
        "method": "join",
        "params": {
             "hash": {
                "method": "uri",
                "uri_position": 1
            },
            "pseudo": {
                "method": "post"
            },
            "playerPosition": {
                "method": "post"
            }
        }
    },
	"playgame": {
		"http_method": "get",
		"type": "page",
		"regexp": "^/play/([a-z0-9]{10}$)",
		"controller": "Game",
		"method": "showPlayPage",
		"params": {
			"hash": {
				"method": "uri",
				"uri_position": 1
			}
		}
	},
    "game_cutdeck": {
        "http_method": "post",
        "type": "api",
        "regexp": "^/play/([a-z0-9]{10})/cutdeck$",
        "controller": "Game",
        "method": "cutDeck",
        "params": {
             "hash": {
                "method": "uri",
                "uri_position": 1
            },
            "value": {
                "method": "post"
            }
        }
    },
    "game_choosetrump": {
        "http_method": "post",
        "type": "api",
        "regexp": "^/play/([a-z0-9]{10})/choosetrump$",
        "controller": "Game",
        "method": "chooseTrump",
        "params": {
             "hash": {
                "method": "uri",
                "uri_position": 1
            },
            "choice": {
                "method": "post"
            }
        }
    }
}